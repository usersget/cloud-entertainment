<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyt.e_music.Dao.MType">

    <select id="selectAll" resultType="org.cyt.entity.Music.Type">
        select * from e_m_type
    </select>
    <select id="selectone" resultType="org.cyt.entity.Music.Type" parameterType="int">
        select * from e_m_type where Typeid=#{Typeid}
    </select>
</mapper>